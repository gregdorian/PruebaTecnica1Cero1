@model WebMVCPrueba.Models.MunicipioTramite

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>


@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>MunicipioTramite</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            @Html.LabelFor(model => model.MunicipioId, "Nombre de Municipio", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("MunicipioId", (IEnumerable<SelectListItem>)ViewBag.Mpios, htmlAttributes: new { @class = "form-control" })
                @*@Html.DropDownList("Mpios", (IEnumerable<SelectListItem>)ViewBag.Mpios);*@
                @Html.ValidationMessageFor(model => model.MunicipioId, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.TramiteId, "Nombre de Tramite", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("TramiteId", (IEnumerable<SelectListItem>)ViewBag.Tram, htmlAttributes: new { @class = "form-control" })
                @*@Html.DropDownList("Tram", (IEnumerable<SelectListItem>)ViewBag.Tram);*@
                @Html.ValidationMessageFor(model => model.TramiteId, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
